# Load Aliases, Oh-My-Zsh, Neovim, Nvm, Pyenv, Taskwarrior & Timewarrior.
source "$HOME/.config/zsh/aliases.zsh"
source "$HOME/.config/zsh/ohmyzsh.zsh"
source "$HOME/.config/zsh/neovim.zsh"
source "$HOME/.config/zsh/npiperelay-ssh-agent.zsh"
source "$HOME/.config/zsh/nvm.zsh"
source "$HOME/.config/zsh/pyenv.zsh"
source "$HOME/.config/zsh/taskwarrior.zsh"
source "$HOME/.config/zsh/timewarrior.zsh"

# Load Angular CLI autocompletion.
source <(ng completion script)

